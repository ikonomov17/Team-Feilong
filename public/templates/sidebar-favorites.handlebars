<div class="favorites">
    <div class="tab-title">
        <h2>Favorites</h2>
    </div>
    <table class="table">
        <thead>
            <tr>
                <th>Index</th>
                <th>Price</th>
                <th>F Score</th>
            </tr>
        </thead>
        <tbody id="favorites-list-table">
            {{#each this}}
            <tr class="favorite-ticket-row">
                <td class="td">{{this.name}}</td>
                <td class="td">{{this.price}}</td>
                <td class="td">{{this.volume}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>