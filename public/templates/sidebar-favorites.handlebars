<div class="favorites">
    <div class="tab-title">
        <h2>Favorites</h2>
    </div>
    <table class="table">
        <thead>
            <tr>
                <th>Index</th>
                <th>Price</th>
                <th>F Score</th>
            </tr>
        </thead>
        <tbody class="favorites-list-table tickets-list-table">
            {{#each this}}
            <tr class="ticket-row" id="">
                <td class="td ticket-name">{{this.name}}</td>
                <td class="td ticket-price">{{this.price}}</td>
                <td class="td ticket-fscore">{{this.fScore}} <span class="close" >Ã—</span></td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>