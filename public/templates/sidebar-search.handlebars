
<div class="row">
    <input type="text" class="input-sm my-input" id="filter-input">
    <ul class="list-group" id="currency-cont"> 
        {{#each name}}
            <li class="list-group-item col-xs-5">
                <label for="{{@index}}">
                    <input type="checkbox" id="{{@index}}"><span class="label-text">{{this}}</span></label>
            </li>
        {{/each}}
    </ul>
</div>
 

